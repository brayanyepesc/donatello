{
    "version": 2,
    "builds": [
        {
            "src": "src/app.svelte",
            "use": "@sveltejs/adapter-auto",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}